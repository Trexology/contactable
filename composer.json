{
    "name": "trexology/contactable",
    "description": "Allows users to have multiple e-mail addresses and phone numbers, and log in with them, on Laravel",
    "autoload": {
        "psr-4": {
            "Trexology\\Contactable\\": "src/"
        }
    },
    "require": {
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "laravel/laravel": "^5.2",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Trexology\\Contactable\\Tests\\": "tests/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Trexology\\Contactable\\ContactableAuthProvider",
          "Trexology\\Contactable\\ContactableServiceProvider"
        ],
        "aliases": {
        }
      }
    }
}
